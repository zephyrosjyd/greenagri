<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>BoardCrawler</servlet-name>
  	<display-name>BoardCrawler</display-name>
  	<description></description>
  	<servlet-class>crawling.BoardCrawler</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProductCrawler</servlet-name>
  	<display-name>ProductCrawler</display-name>
  	<description></description>
  	<servlet-class>crawling.ProductCrawler</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BoardCrawler</servlet-name>
  	<url-pattern>/crawling/board</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProductCrawler</servlet-name>
  	<url-pattern>/crawling/product</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
